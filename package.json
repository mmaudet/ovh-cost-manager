{
  "name": "ovh-bill",
  "description": "OVH bills downloader and cost dashboard",
  "version": "2.0.0",
  "private": true,
  "homepage": "https://github.com/somanos/ovh-bill",
  "author": "Somanos Sar <somanos@drumee.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/somanos/ovh-bill"
  },
  "bugs": {
    "url": "https://github.com/somanos/ovh-bill/issues"
  },
  "license": "MIT",
  "workspaces": [
    "cli",
    "data",
    "server",
    "dashboard"
  ],
  "scripts": {
    "cli": "node cli/index.js",
    "split": "node cli/split-by-project.js",
    "import": "node data/import.js",
    "import:full": "node data/import.js --full",
    "import:diff": "node data/import.js --diff",
    "dev:server": "node server/index.js",
    "dev:dashboard": "npm run dev --workspace=dashboard",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:dashboard\"",
    "build": "npm run build --workspace=dashboard",
    "start": "npm run dev:server"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
